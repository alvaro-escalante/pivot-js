{
  "name": "pivot-table-js",
  "version": "1.0.1",
  "typings": "@types/declarations.d.ts",
  "description": "A lightweight module that takes an array of objects and produces an array of objects based on the aggregate values. Emulating excel pivot tables.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "watch": "jest --watchAll",
    "build": "tsc",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "issues": "https://github.com/alvaro-escalante/pivot-js/issues",
    "url": "git+https://github.com/alvaro-escalante/pivot-js.git"
  },
  "keywords": [
    "Pivot",
    "Pivot Table",
    "Pivot Table JS"
  ],
  "author": "Alvaro Fernandez",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "jest": "^27.2.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.3"
  },
  "files": [
    "lib/**/*"
  ]
}